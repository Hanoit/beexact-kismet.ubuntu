# ==========================================
# CONFIGURACIÓN COMPLETA KISMET PROCESSOR
# ==========================================

# DIRECTORIOS
WATCH_DIRECTORY=/opt/kismetFiles
OUT_DIRECTORY=/opt/kismetFiles

# CONFIGURACIÓN MACVENDORS API - OPTIMIZADA
MACVENDOR_PLAN_TYPE=premium
MACVENDOR_REQUESTS_PER_SECOND=25
MACVENDOR_REQUESTS_PER_DAY=100000
MACVENDOR_API_TIMEOUT=8.0
MACVENDOR_API_INTERVAL=0.04
API_KEY_MACVENDOR=

# BATCH PROCESSING OPTIMIZADO
MACVENDOR_BATCH_SIZE=25
MACVENDOR_MAX_WORKERS=25
MACVENDOR_BATCH_TIMEOUT=35.0
MACS_NOT_FOUND_CACHE_MONTHS=6

# PROCESAMIENTO DE ARCHIVOS
FILE_QUEUE_MAX_SIZE=20
CHECK_INTERVAL=300
KISMET_CHUNK_SIZE=10000

# MONITOREO Y PERFORMANCE
ENABLE_PERFORMANCE_MONITOR=true
ENABLE_PROGRESS_BAR=1
SAVE_INTERMEDIATE_RESULTS=1

# PROCESAMIENTO DE UBICACIÓN
PROCESS_WITHOUT_LOCATION=1
FLIP_XY=0

# LOGGING Y VERBOSIDAD
ADVANCE_VERBOSE=0
BASIC_VERBOSE=0

# BASE DE DATOS

# BASE DE DATOS - CONFIGURACIÓN DE REINTENTOS
DB_RETRY_ATTEMPTS=3
DB_RETRY_DELAY=0.1

# CONFIGURACIONES ADICIONALES DE PROCESAMIENTO
SEQUENTIAL_ID_THREAD_MOD=10000
DEVICE_JSON_INDEX=14
COORDINATE_PRECISION=6
COORDINATE_DECIMAL_PLACES=4
DISTANCE_FILTER_METERS=50
COORDINATE_EPSG=4326
PROJECTED_EPSG=3857
PROGRESS_BAR_WIDTH=100
CONSOLE_SEPARATOR_WIDTH=50
CONSOLE_PROGRESS_WIDTH=40
TARGET_DEVICES_PER_SECOND=25

# BASE DE DATOS
DB_PATH=kismet.db
